{
  "name": "shu-ho-standalone",
  "version": "0.0.1",
  "description": "Shu-ho, a weekly-report editing application",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-devserver --inline --hot",
    "bootlint": "/bin/cat index.html | ./node_modules/.bin/bootlint ",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "build-v": "./node_modules/.bin/webpack --display-error-details --config webpack.config.js",
    "package": "./node_modules/.bin/electron-packager . --overwrite  --prune=true --ignore node_modules --electron-version=1.6.1 --out=release",
    "start": "./node_modules/.bin/electron .",
    "test": ""
  },
  "author": "Hiroki Ito <hiroki110@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bootlint": "^0.14.2",
    "css-loader": "^0.26.2",
    "devtron": "^1.4.0",
    "electron": "^1.6.1",
    "electron-packager": "^8.5.2",
    "electron-reload": "^1.1.0",
    "fs": "0.0.1-security",
    "html-loader": "^0.4.5",
    "nodemailer": "^3.1.4",
    "path": "^0.12.7",
    "style-loader": "^0.13.2",
    "url": "^0.11.0",
    "vue": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-devserver": "0.0.6"
  },
  "main": "main.js"
}
