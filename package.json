{
  "name": "shu-ho-standalone",
  "version": "0.0.4",
  "description": "Shu-ho, a weekly-report editing application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrkt/shu-ho-standalone.git"
  },
  "scripts": {
    "bootlint": "/bin/cat index.html | ./node_modules/.bin/bootlint ",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "build-v": "./node_modules/.bin/webpack --display-error-details --config webpack.config.js",
    "build-w": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "clean": "./node_modules/.bin/npm-run-all -s release:clean",
    "count": "./node_modules/.bin/sloc --details --format cli-table --keys total,source,comment --format-option no-head src/",
    "dev": "NODE_ENV=develop ./node_modules/.bin/electron .",
    "lint": "./node_modules/.bin/eslint src/js",
    "release:mac": "./node_modules/.bin/electron-packager ./ --platform=darwin --overwrite",
    "release:clean": "./node_modules/.bin/rimraf ./dist",
    "start": "./node_modules/.bin/electron .",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register",
    "version": "ELECTRON_RUN_AS_NODE=1 ./node_modules/.bin/electron -e 'process.stdout.write(process.versions.modules)'"
  },
  "author": "Hiroki Ito <hiroki110@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "babel-preset-es2015": "^6.24.0",
    "bootlint": "^1.1.0",
    "bootstrap": "^5.1.3",
    "brace": "^0.11.1",
    "chai": "^4.3.4",
    "css-loader": "^6.5.1",
    "devtron": "^1.4.0",
    "electron": ">=16.0.7",
    "electron-packager": "^15.4.0",
    "electron-reload": "^1.5.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-vue": "^8.3.0",
    "fs": "0.0.1-security",
    "html-loader": "^3.1.0",
    "jquery": "^3.6.0",
    "marked": "^4.0.10",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.4",
    "moment": "^2.29.1",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "remote": "^0.2.6",
    "rimraf": "^3.0.2",
    "sloc": "^0.2.1",
    "style-loader": "^3.3.1",
    "url": "^0.11.0",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.66.0"
  },
  "main": "main.js",
  "node": {
    "fs": "empty"
  }
}
