{
  "name": "shu-ho-standalone",
  "version": "0.0.2",
  "description": "Shu-ho, a weekly-report editing application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrkt/shu-ho-standalone.git"
  },
  "scripts": {
    "bootlint": "/bin/cat index.html | ./node_modules/.bin/bootlint ",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "build-w": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "build-v": "./node_modules/.bin/webpack --display-error-details --config webpack.config.js",
    "clean": "./node_modules/.bin/npm-run-all -s release:clean",
    "dev": "NODE_ENV=develop ./node_modules/.bin/electron .",
    "lint": "./node_modules/.bin/eslint js",
    "release:clean": "./node_modules/.bin/rimraf ./dist",
    "start": "./node_modules/.bin/electron .",
    "test": "",
    "version": "ELECTRON_RUN_AS_NODE=1 ./node_modules/.bin/electron -e 'process.stdout.write(process.versions.modules)'"
  },
  "author": "Hiroki Ito <hiroki110@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bootlint": "^0.14.2",
    "brace": "^0.10.0",
    "css-loader": "^0.26.2",
    "devtron": "^1.4.0",
    "electron": "^1.6.1",
    "electron-packager": "^8.5.2",
    "electron-reload": "^1.1.0",
    "eslint": "^3.17.1",
    "fs": "0.0.1-security",
    "html-loader": "^0.4.5",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "url": "^0.11.0",
    "vue": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "main": "main.js",
  "node": {
    "fs": "empty"
  }
}
